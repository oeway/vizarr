{
  "name": "@hms-dbmi/vizarr",
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run check && vite build",
    "preview": "vite preview",
    "export": "npm run check && npm run build",
    "format": "prettier --write \"{src,types}/**/*.{js,jsx,ts,tsx}\" vite.config.js",
    "test": "playwright test",
    "test:core": "playwright test zarr-core-functionality.spec.ts",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test",
    "lint": "biome ci",
    "fix": "biome check --write",
    "check": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hms-dbmi/viv": "~0.17.2",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/styles": "^5.15.10",
    "@vivjs/types": "~0.17.0",
    "@zarrita/storage": "0.1.0",
    "deck.gl": "~9.0.0",
    "hypha-core": "0.20.56-pre6",
    "hypha-rpc": "^0.20.55",
    "jotai": "^1.0.0",
    "just-debounce-it": "^3.1.1",
    "math.gl": "^4.1.0",
    "p-map": "^7.0.3",
    "quick-lru": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zarrita": "0.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@danmarshall/deckgl-typings": "^4.3.10",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.2.0",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.2",
    "vite": "^6.2.7"
  },
  "packageManager": "pnpm@9.5.0",
  "pnpm": {
    "patchedDependencies": {
      "@vivjs/loaders@0.17.2": "patches/@vivjs__loaders@0.17.2.patch"
    }
  }
}
